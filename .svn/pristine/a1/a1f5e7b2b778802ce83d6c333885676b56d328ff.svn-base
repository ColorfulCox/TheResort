.tablestyle {
	font-family: "ו";
	background-color: #3300FF;
	background-image: url(../images/login_logo.jpg);
	background-repeat: repeat-x;
	border-style: 0;
}
#a,#b {
	height: 200px;
	width: 200px;
	float: left;
	margin-top: 50px;
	border: 1px solid #990000;
}
#b {
	border: 1px solid #990000;
	position: relative;	
	margin-left: 50px;
}
#c,#d {
	height: 50%;
	width: 50%;
	position: absolute;
	top: 50%;
	left: 50%;
	border: 1px solid #FF00FF;
	padding: 1px;
}
